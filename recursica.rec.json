{
  "name": "Recursica Material UI SANDBOX",
  "imports": [
    "./src/theme/theme.rec.json",
    "./src/theme/colors.rec.json",
    "./src/theme/typography.rec.json",
    "./src/components/Button/button.rec.json"
  ],
  "icons": {},
  "theme": {
    "config": {
      "element": {
        "unit": "rem",
        "ratio": 1.6
      },
      "text": {
        "unit": "rem",
        "ratio": 1.6
      }
    },
    "colors": {
      "black": "#000",
      "white": "#fff",
      "primary": {
        "main": "#1976d2",
        "light": "#42a5f5",
        "dark": "#1565c0",
        "contrastText": "#fff"
      },
      "secondary": {
        "main": "#9c27b0",
        "light": "#ba68c8",
        "dark": "#7b1fa2",
        "contrastText": "#fff"
      },
      "error": {
        "main": "#d32f2f",
        "light": "#ef5350",
        "dark": "#c62828",
        "contrastText": "#fff"
      },
      "warning": {
        "main": "#ed6c02",
        "light": "#ff9800",
        "dark": "#e65100",
        "contrastText": "#fff"
      },
      "info": {
        "main": "#0288d1",
        "light": "#03a9f4",
        "dark": "#01579b",
        "contrastText": "#fff"
      },
      "success": {
        "main": "#2e7d32",
        "light": "#4caf50",
        "dark": "#1b5e20",
        "contrastText": "#fff"
      },
      "grey": {
        "50": "#fafafa",
        "100": "#f5f5f5",
        "200": "#eee",
        "300": "#e0e0e0",
        "400": "#bdbdbd",
        "500": "#9e9e9e",
        "600": "#757575",
        "700": "#616161",
        "800": "#424242",
        "A100": "#212121",
        "A200": "#eee",
        "A400": "#bdbdbd",
        "A700": "#616161"
      },
      "blue": {
        "50": "#E3F2FD",
        "100": "#BBDEFB",
        "200": "#90CAF9",
        "300": "#64B5F6",
        "400": "#42A5F5",
        "500": "#2196F3",
        "600": "#1E88E5",
        "700": "#1976D2",
        "800": "#0D47A1",
        "A100": "#82B1FF",
        "A200": "#448AFF",
        "A400": "#2979FF",
        "A700": "#f6fafd",
        "A7005": "#8cbae8"
      }
    },
    "typography": {}
  },
  "components": {
    "MuiButton": {
      "label": "Button",
      "styles": {
        "display": "inline-flex",
        "align-items": "center",
        "justify-content": "center",
        "position": "relative",
        "box-sizing": "border-box",
        "outline": "0",
        "border": "0",
        "margin": "0",
        "cursor": "pointer",
        "user-select": "none",
        "vertical-align": "middle",
        "text-decoration": "none",
        "font-family": "'Roboto','Helvetica','Arial,sans-serif'",
        "font-weight": "500",
        "font-size": "0.875rem",
        "line-height": "1.75",
        "letter-spacing": "0.02857em",
        "text-transform": "uppercase",
        "min-width": "64px",
        "border-radius": "4px",
        "transition": "background-color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms,box-shadow 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms,border-color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms,color 250ms cubic-bezier(0.4, 0, 0.2, 1) 0ms",
        "padding": "6px 8px",
        "color": "{theme.colors.blue.700}",
        "background-color": "transparent"
      },
      "properties": {
        "hover": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        },
        "disabled": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        },
        "focus": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        },
        "active": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        },
        "variant": {
          "_normalized": true,
          "default": "text",
          "values": [
            "text",
            "contained",
            "outlined"
          ],
          "type": "string"
        },
        "size": {
          "_normalized": true,
          "default": "medium",
          "values": [
            "medium",
            "small",
            "large"
          ],
          "type": "string"
        },
        "disableElevation": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        },
        "fullWidth": {
          "_normalized": true,
          "default": "false",
          "values": [
            "false",
            "true"
          ],
          "type": "boolean"
        }
      },
      "variants": {
        "variant=contained,hover=true": {
          "styles": {
            "background": "{theme.colors.primary.dark}",
            "box-shadow": "0px 2px 4px -1px rgb(0 0 0 / 20%), 0px 4px 5px 0px rgb(0 0 0 / 14%), 0px 1px 10px 0px rgb(0 0 0 / 12%)"
          }
        },
        "variant=contained,active=true": {
          "styles": {
            "box-shadow": "0px 5px 5px -3px rgb(0 0 0 / 20%), 0px 8px 10px 1px rgb(0 0 0 / 14%), 0px 3px 14px 2px rgb(0 0 0 / 12%)"
          }
        },
        "variant=contained,disabled=true": {
          "styles": {
            "color": "{theme.colors.grey.400}",
            "box-shadow": "none",
            "background-color": "{theme.colors.grey.300}"
          }
        },
        "variant=outlined,hover=true": {
          "styles": {
            "background-color": "{theme.colors.blue.A700}",
            "border": "1px solid {theme.colors.blue.700}"
          }
        },
        "variant=outlined,disabled=true": {
          "styles": {
            "color": "{theme.colors.grey.400}",
            "border": "1px solid {theme.colors.grey.300}"
          }
        }
      },
      "name": "Mui Button",
      "rules": {
        "hover=true": {
          "styles": {
            "text-decoration": "none",
            "background-color": "{theme.colors.blue.A700}"
          }
        },
        "disabled=true": {
          "styles": {
            "color": "{theme.colors.grey.400}",
            "pointer-events": "none",
            "cursor": "default"
          }
        },
        "disableElevation=true": {
          "styles": {
            "box-shadow": "none"
          }
        },
        "fullWidth=true": {
          "styles": {
            "width": "100%"
          }
        },
        "variant=outlined": {
          "styles": {
            "padding": "5px 15px",
            "color": "{theme.colors.blue.700}",
            "border": "1px solid {theme.colors.blue.A7005}"
          }
        },
        "size=small": {
          "styles": {
            "font-size": "0.8125rem",
            "padding": "4px 10px"
          }
        },
        "size=large": {
          "styles": {
            "font-size": "0.9375rem",
            "padding": "8px 22px"
          }
        },
        "variant=contained": {
          "styles": {
            "padding": "6px 16px",
            "color": "{theme.colors.white}",
            "box-shadow": "0px 3px 1px -2px rgb(0 0 0 / 20%), 0px 2px 2px 0px rgb(0 0 0 / 14%), 0px 1px 5px 0px rgb(0 0 0 / 12%)",
            "background-color": "rgba(236, 31, 31, 1.00)"
          }
        }
      }
    }
  },
  "revision": 2
}